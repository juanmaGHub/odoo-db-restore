# Odoo backup environment variables (typically from production)
ODOO_HOST_NAME=myhost.com
ODOO_HOST_PORT=8069 (or empty)
ODOO_HOST_PROTOCOL=https
ODOO_DB_NAME=db

# Odoo restore environment variables (typically from test)
ODOO_TARGET_DB_NAME=db-test
ODOO_MASTER_PASSWORD=admin_passwd
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_PASSWORD=admin
FILESTORE_PATH=/home/odoo/.local/share/Odoo/filestore/